{
  "version": "2.0.5",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "lint": "tsdx lint src",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint src"
    }
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "@typescript-eslint/no-redeclare": "off",
      "no-redeclare": "off"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "sanity-plugin-transifex",
  "author": "Sanity.io",
  "module": "dist/sanity-plugin-transifex.esm.js",
  "homepage": "https://github.com/sanity-io/sanity-plugin-transifex#readme",
  "size-limit": [
    {
      "path": "dist/sanity-plugin-transifex.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/sanity-plugin-transifex.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@sanity/types": "^2.11.0",
    "eslint-config-react-app": "^6.0.0",
    "husky": "^4.3.0",
    "size-limit": "^4.7.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "sanity-translations-tab": "^2.0.5"
  }
}
